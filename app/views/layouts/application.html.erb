<%= render :file => "users/user_panel" %>
<!DOCTYPE html>
<html>
<head>
  <title>Watch1t</title>
  <%= stylesheet_link_tag "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<header class="navbar navbar-fixed-top navbar-inverse">
		<div class="container">
            <div class="navbar-header">
                    <%= link_to "Watch1t", root_url, id: "logo" %>
            </div>
            <div>
					<ul class="nav navbar-nav navbar-right">
						<li class=""><%= link_to "Contacts", "/users/send_contact_mail"%></li>
						<li class=""><%= link_to "Search", "/"%></li>
					</ul>
            </div>
        </div>
	</header>
	<section class="container">
		<div class="row">

			<div class="col-md-8">
				<%= yield %>
			</div>

			<div class="col-md-4">
				<%= yield :user_panel %>
			</div>
		</div>
	</section>
	<footer>
		
	</footer>
</body>
</html>
